{
  "name": "banka-ui-react",
  "version": "1.0.0",
  "description": "The UI interface for the banka App",
  "main": "index.js",
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "start": "node ./dist/server.js",
    "dev": "webpack-dev-server --mode development --config webpack.dev.config.js --open",
    "build": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "clean": "rm -rf build && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvjude/Banka-UI-React.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Jude Chinoso",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cvjude/Banka-UI-React/issues"
  },
  "homepage": "https://github.com/cvjude/Banka-UI-React#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "CI=true npm test"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
